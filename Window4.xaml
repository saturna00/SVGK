        <TextBlock x:Name="textblock5" TextWrapping="Wrap" Text="Трудоустройство" Margin="10,267,349,172" FontSize="18" Foreground="White"/>
        <TextBlock x:Name="textblock6" TextWrapping="Wrap" Text="Сумма задолженности" Margin="10,320,349,125" FontSize="18" Foreground="White"/>
        <TextBox x:Name="_6" HorizontalAlignment="Left" Height="40" Margin="221,320,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="420" Background="{x:Null}" Foreground="White" FontSize="18"/>
        <TextBox x:Name="_5" HorizontalAlignment="Left" Height="40" Margin="221,273,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="420" Background="{x:Null}" Foreground="White" FontSize="18"/>
        <TextBox x:Name="_4" HorizontalAlignment="Left" Height="40" Margin="221,229,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="420" Background="{x:Null}" Foreground="White" FontSize="18"/>
        <TextBox x:Name="_3" HorizontalAlignment="Left" Height="40" Margin="221,182,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="420" Background="{x:Null}" Foreground="White" FontSize="18"/>
        <TextBox x:Name="_2" HorizontalAlignment="Left" Height="40" Margin="221,138,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="420" Background="{x:Null}" Foreground="White" FontSize="18"/>
        <TextBox x:Name="_1" HorizontalAlignment="Left" Height="40" Margin="221,91,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="420" Background="{x:Null}" Foreground="White" FontSize="18"/>
        <Button x:Name="vnos" Content="Внести данные " HorizontalAlignment="Left" Height="44" Margin="465,365,0,0" VerticalAlignment="Top" Width="174" Background="#FF8F8F8F" Foreground="White" FontSize="18" Click="vnos_Click"/>
